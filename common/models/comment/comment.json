{
  "name": "Comment",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true
  },
  "properties": {
    "content": {
      "type": "string"
    },
    "posts": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "userId"
    },
    "signal": {
      "type": "belongsTo",
      "model": "Signal",
      "foreignKey": "modelId"
    },
    "news": {
      "type": "belongsTo",
      "model": "News",
      "foreignKey": "modelId"
    }
  },
  "acls": [],
  "methods": {}
}
