{
  "name": "Advisory",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true,
    "Posts": true
  },
  "properties": {
    "title": "string",
    "location": "string",
    "modality": "string",
    "thematic": "string",
    "coachTerms": "string",
    "additionalAdvantages": "string",
    "cost": "number",
    "language": "string",
    "hours": "number",
    "aboutCoach": "string"
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": ""
    },
    "coin": {
      "type": "belongsTo",
      "model": "Coin",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    },
    "complaint": {
      "type": "hasMany",
      "model": "Complaint",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    }
  },
  "acls": [],
  "methods": {}
}