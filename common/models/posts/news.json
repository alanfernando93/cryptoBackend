{
  "name": "News",
  "base": "PersistedModel",
  "idInjection": false,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true,
    "Posts": true
  },
  "properties": {
    "title": "string",
    "guessCoin": {
      "type": "string"
    },
    "guessPrice": {
      "type": "string"
    },
    "sources": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "Comment": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "userId",
      "through": "Comment"
    },
    "User": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}  
}
