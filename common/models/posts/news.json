{
  "name": "News",
  "base": "PersistedModel",
  "idInjection": false,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true,
    "Posts": true
  },
  "properties": {
    "title": "string",
    "guessCoin": {
      "type": "string"
    },
    "guessPrice": {
      "type": "string"
    },
    "sources": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": ""
    },
    "coin": {
      "type": "belongsTo",
      "model": "Coin",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    },
    "complaint": {
      "type": "hasMany",
      "model": "Complaint",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    }
  },
  "acls": [],
  "methods": {}
}