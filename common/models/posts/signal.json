{
  "name": "Signal",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true,
    "Posts": true
  },
  "properties": {
    "offer": {
      "type": "boolean",
      "required": true
    },
    "investmentType": {
      "type": "string",
      "required": false
    },
    "fundamentalAnalysis": {
      "type": "string",
      "required": false,
      "dataType": "text"
    },
    "technicalAnalysis": {
      "type": "string",
      "required": false,
      "dataType": "text"
    },
    "exitStrategy": {
      "type": "string",
      "required": false,
      "dataType": "text"
    },
    "tags": {
      "type": "string",
      "required": false
    },
    "precision": {
      "type": "number",
      "required": false
    },
    "currencyExchange": {
      "type": "string",
      "required": false
    },
    "exchange": {
      "type": "string",
      "required": false
    },
    "PEP": {
      "type": "string",
      "required": false
    },
    "PSP": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": ""
    },
    "coin": {
      "type": "belongsTo",
      "model": "Coin",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    },
    "complaint": {
      "type": "hasMany",
      "model": "Complaint",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    }
  },
  "acls": [],
  "methods": {}
}