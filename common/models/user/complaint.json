{
  "name": "Complaint",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true
  },
  "properties": {
    "type": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    },
    "posts": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "news": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    },
    "signal": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelName"
      }
    }
  },
  "acls": [],
  "methods": {}
}
