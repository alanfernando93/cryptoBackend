{
  "name": "Usuario",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "restrictResetPasswordTokenScope": true,
  "emailVerificationRequired": true,
  "properties": {
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "socialNetworks": {
      "type": [
        "object"
      ],
      "required": false,
      "default": "[]"
    },
    "fame": {
      "type": [
        "object"
      ],
      "requiered": false,
      "default": "[]"
    },
    "brithdate" : {
      "type": "date"
    },
    "points": {
      "type": "number"
    },
    "precision": {
      "type": "object",
      "default": {
        "valor": 0,
        "nropost": 0
      }
    },
    "visit": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "Signal": {
      "type": "hasMany",
      "model": "Signal",
      "foreignKey": "userId",
      "through": "Comment"
    },
    "News": {
      "type": "hasMany",
      "model": "News",
      "foreignKey": "userId",
      "through": "Comment"
    },
    "Comment": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "userId",
      "through": "Answer"
    }
  },
  "acls": [],
  "methods": {}
}